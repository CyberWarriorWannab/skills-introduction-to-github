def binary_search(arr: list[int], target: int) -> int: 
"""Return index of target in sorted arr, or -1 if not found.""" 

    lo, hi = 0, len(arr) - 1 

    while lo <= hi: 
      mid = lo + (hi - lo) // 2 
      if arr[mid] == target: 
        return mid 
      if arr[mid] < target: 
        lo = mid + 1 
      else: 
        hi = mid - 1 
    return -1
